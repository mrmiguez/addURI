  <titleInfo lang="eng">
    <title>Ethics of Organ Allocation</title>
    <subTitle>A Consequentialist Approach</subTitle>
  </titleInfo>
  <name authority="local" type="personal">
    <namePart type="given">Danae</namePart>
    <namePart type="family">Massengill</namePart>
